[type="radio"]:checked,
[type="radio"]:not(:checked) {
  position: absolute;
  left: -9999px; }

[type="radio"]:checked + label,
[type="radio"]:not(:checked) + label {
  position: relative;
  padding-left: 50px;
  cursor: pointer;
  line-height: 20px;
  display: inline-block;
  color: #666; }

[type="radio"]:checked + label:before,
[type="radio"]:not(:checked) + label:before {
  content: '';
  position: absolute;
  left: 0;
  top: 8px;
  width: 34px;
  height: 34px;
  border: 5px solid #52E5E7;
  border-radius: 100%; }

[type="radio"]:checked + label:after,
[type="radio"]:not(:checked) + label:after {
  content: '';
  width: 34px;
  height: 34px;
  background: #fbfcff;
  position: absolute;
  top: 8px;
  left: 0;
  border: 5px solid #52E5E7;
  border-radius: 100%;
  -webkit-transition: all 0.2s ease;
  transition: all 0.2s ease; }

[type="radio"]:not(:checked) + label:after {
  opacity: 0;
  -webkit-transform: scale(0);
  transform: scale(0); }

[type="radio"]:checked + label:after {
  opacity: 1;
  -webkit-transform: scale(1);
  transform: scale(1); }

.popup-progressbar {
  opacity: 0;
  max-height: 0;
  overflow: hidden;
  font-size: 16px;
  transform: scale(0.8); }
  input[type="radio"]:checked ~ .popup-progressbar {
    opacity: 1;
    max-height: 100%;
    margin: 30px 0;
    transform: scale(1);
    overflow: visible; }

.modal-open {
  overflow-y: scroll; }

